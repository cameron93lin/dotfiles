# Zsh Config
alias zshconfig="code $ZDOTDIR/.zshrc"
alias zshs="source $ZDOTDIR/.zshrc"

# File System
alias ls='ls -G'
alias tree='tree -N'

# Homebrew
alias xbrew="arch -x86_64 /usr/local/bin/brew"
alias abrew="/opt/homebrew/bin/brew"

# SVN
alias svn=/Library/Developer/CommandLineTools/usr/bin/svn
