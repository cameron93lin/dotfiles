ki() {
    kinit -f && date +%s > ~/.kinit_ts
}
 
check_kinit() {
    local TS KIN KDIFF
    TS=$(date +%s)

    read -r KIN < ~/.kinit_ts || KIN=0
    [[ "$KIN" =~ ^[0-9]+$ ]] || KIN=0

    KDIFF=$((TS - KIN))

    if (( KDIFF > 21600 )); then
        echo "KINIT expired, renewing cert..."
        ki
    fi
}
mw() {
    local PIN OTP

    PIN=$(security find-generic-password \
        -a "$USER" \
        -s "mwinit-pin" \
        -w) || {
        echo "Failed to retrieve PIN from Keychain"
        return 1
    }

    read "OTP?Yubikey: "

    if mwinit -o <<EOF
$PIN
$OTP
EOF
    then
        date +%s > ~/.mwinit_ts
    else
        echo "mwinit failed; not updating timestamp"
        return 1
    fi

    unset PIN OTP
}

 check_mwinit() {
    local TS MWIN MWDIFF
    TS=$(date +%s)

    read -r MWIN < ~/.mwinit_ts || MWIN=0
    [[ "$MWIN" =~ ^[0-9]+$ ]] || MWIN=0

    MWDIFF=$((TS - MWIN))

    if (( MWDIFF > 43200 )); then
        echo "MWINIT expired, renewing sign..."
        mw
    fi
}

_check_inits() {
    check_kinit
    check_mwinit
}
_check_inits
 
ut() {
    eval `ssh-agent -s`
    ssh-add ~/.ssh/id_rsa
    ki
    mw
}
